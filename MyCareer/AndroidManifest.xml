        android:theme="@style/AppTheme" >
        <activity
            android:name="com.societegenerale.hackaton.mycareer.ui.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.societegenerale.hackaton.mycareer.ui.activities.SkillsFragmentActivity"
            android:label="@string/My_Skils" >
        </activity>
    </application>

</manifest>